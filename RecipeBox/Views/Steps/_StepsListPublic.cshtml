@model List<Step>

<ol class="list-group list-group-numbered">
@if(Model.Count == 0)
{
  <li class="list-group-item d-flex justify-content-between align-items-center">No Steps</li>
}
@foreach (Step s in Model)
{
  <li class="list-group-item d-flex justify-content-between align-items-center">
    @s.Description
  </li>
}
</ol>
